{
  "root": "dist",
  "clean_urls": true,
  "routes": {
    "/index.html": "/",
    "/assets/**": "/assets/:splat",
    "/vite.svg": "/vite.svg",
    "/favicon.ico": "/favicon.ico",
    "/**": "index.html"
  },
  "headers": {
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/index.html": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    }
  },
  "redirects": {
    "/index.html": {
      "url": "/",
      "status": 301
    }
  }
}
