{
  "version": 2,
  "builds": [
    {
      "src": "backend/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "backend/index.js"
    }
  ],
  "functions": {
    "backend/middleware/edgeMiddleware.js": {
      "runtime": "edge"
    }
  }
} 